all: door_struct list_test stack_test

door_struct: dmanager_module.o
	gcc dmanager_module.o -o ../Build/Quest_1

list_test: list.o list_test.o
	gcc list.o list_test.o -o ../Build/Quest_2

stack_test: stack.o stack_test.o
	gcc stack.o stack_test.o -o ../Build/Quest_2

rebuild: clean door_struct list_test stack_test

dmanager_module.o:
	gcc -Wall -Werror -Wextra -c dmanager_module.c

list.o:
	gcc -Wall -Werror -Wextra -c dmanager_module.c

list_test.o:
	gcc -Wall -Werror -Wextra -c dmanager_module.c

stack.o:
	gcc -Wall -Werror -Wextra -c dmanager_module.c

stack_test.o:
	gcc -Wall -Werror -Wextra -c dmanager_module.c

clean:
	-rm ../Build/Quest_1 ../Build/Quest_2 ../Build/Quest_3
	-rm stack_test.o stack.o list_test.o list.o dmanager_module.o
